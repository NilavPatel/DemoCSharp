@model MVCPartialView.Models.Employee
@{
    ViewBag.Title = "Summary";
}

<h2>Summary</h2>

<div class="form-horizontal">
    <div class="form-group">
        @Html.LabelFor(model => model.Personal.Name, htmlAttributes: new { @class = "control-EditorFor col-md-2" })
        <div class="col-md-10">
            @(Model.Personal.Name)
        </div>
    </div>
    <div class="form-group">
        @Html.LabelFor(model => model.Personal.BirthDate, htmlAttributes: new { @class = "control-EditorFor col-md-2" })
        <div class="col-md-10">
            @(Model.Personal.BirthDate)
        </div>
    </div>
    <div class="form-group">
        @Html.LabelFor(model => model.Personal.ContactNumber, htmlAttributes: new { @class = "control-EditorFor col-md-2" })
        <div class="col-md-10">
            @(Model.Personal.ContactNumber)
        </div>
    </div>
</div>
