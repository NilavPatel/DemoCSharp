        @Html.LabelFor(model => model.Personal.ContactNumber, htmlAttributes: new { @class = "control-EditorFor col-md-2" })
        <div class="col-md-10">
            @(Model.Personal.ContactNumber)
        </div>
    </div>
</div>
<hr />
